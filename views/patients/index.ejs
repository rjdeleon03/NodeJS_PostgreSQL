<% include ../partials/header %>
<div class="ui container">
    <h1>Patients page</h1>
    
    <table class="ui celled striped table">
        <thead>
            <th>Name</th>
            <th>Age</th>
            <th>Location</th>
        </thead>
        <tbody>
            <% patients.forEach(function(patient) { %>
                <tr>
                    <td><%= patient.name %></td>
                    <td><%= patient.age %></td>
                    <td><%= patient.location %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</div>    
<% include ../partials/footer %>